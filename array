#include<iostream>
using namespace std;
main()
{
	static int a[5]={1,2,3,4,5};
	int i ,n ,temp;
	n=5;
	cout<<"enter element:"<<endl;
	for(i=0;i<=n-1;++i)
	{
		temp=*((a)+(i));
		cout<<" "<<temp<<endl;
	}
	cout<<"you entered:"<<endl;
	for(i=0;i<=n-1;++i)
	{
		temp=*((a)+(i));
		cout<<" "<<temp<<endl;
	}
	return 0;
}
